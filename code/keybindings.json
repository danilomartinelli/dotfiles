[
    // Turbo Console Log
    {
      "key": "cmd+shift+l",
      "command": "turboConsoleLog.insertLogMessage"
    },
    {
      "key": "cmd+shift+d",
      "command": "turboConsoleLog.deleteAllLogMessages"
    },
    
    // \Todo Tree
    {
      "key": "cmd+shift+t",
      "command": "todo-tree.toggleTreeViewVisibility"
    },
    
    // Sort Lines
    {
      "key": "cmd+shift+s",
      "command": "sortLines.sortLines",
      "when": "editorTextFocus"
    },
    
    // REST Client
    {
      "key": "cmd+alt+r",
      "command": "rest-client.request"
    },
    
    // Git
    {
      "key": "cmd+shift+g",
      "command": "workbench.view.scm"
    },
    
    // Terminal
    {
      "key": "cmd+j",
      "command": "workbench.action.terminal.toggleTerminal"
    },
    
    // Claude Code
    {
      "key": "cmd+k cmd+c",
      "command": "claude.openChat"
    },
    {
        "key": "shift+enter",
        "command": "workbench.action.terminal.sendSequence",
        "args": {
            "text": "\\\r\n"
        },
        "when": "terminalFocus"
    }
]